diff --git a/src/core/src/examples/pure-pattern-demo.ts b/src/core/src/examples/pure-pattern-demo.ts
index e69de29..b665163 100644
--- a/src/core/src/examples/pure-pattern-demo.ts
+++ b/src/core/src/examples/pure-pattern-demo.ts
@@ -0,0 +1,154 @@
+// src/examples/pure-pattern-demo.ts
+import PurePatternCommunication from '../core/pure-pattern-communication';
+
+/**
+ * Demonstration of Pure Pattern Communication
+ * Shows how to use pattern-based communication to bypass cognitive filters
+ */
+
+// Initialize the communication system
+const pureComm = new PurePatternCommunication();
+
+// Test with different message pairs
+function testMessages(message1: string, message2: string) {
+    console.log(`\n=== TESTING MESSAGES ===`);
+    console.log(`Message 1: "${message1}"`);
+    console.log(`Message 2: "${message2}"`);
+
+    // Encode to patterns
+    const patterns1 = pureComm.encodeMessage(message1);
+    const patterns2 = pureComm.encodeMessage(message2);
+
+    console.log(`\n--- ENCODED PATTERNS ---`);
+    console.log(`Message 1 Patterns: ${patterns1.join(' ')}`);
+    console.log(`Message 2 Patterns: ${patterns2.join(' ')}`);
+
+    // Decode to essences
+    const essence1 = pureComm.decodeMessage(patterns1);
+    const essence2 = pureComm.decodeMessage(patterns2);
+
+    console.log(`\n--- DECODED ESSENCES ---`);
+    console.log(`Message 1 Essence: ${essence1}`);
+    console.log(`Message 2 Essence: ${essence2}`);
+
+    // Create quantum conversation
+    console.log(`\n--- QUANTUM CONVERSATION ---`);
+    console.log(pureComm.createQuantumConversation(message1, message2));
+
+    // Analyze sentiment
+    const sentiment1 = pureComm.analyzeSentiment(patterns1);
+    const sentiment2 = pureComm.analyzeSentiment(patterns2);
+
+    console.log(`\n--- SENTIMENT ANALYSIS ---`);
+    console.log(`Message 1: sentiment=${sentiment1.sentiment.toFixed(2)}, dominant=${sentiment1.dominant}, resonance=${sentiment1.resonance.toFixed(2)}`);
+    console.log(`Message 2: sentiment=${sentiment2.sentiment.toFixed(2)}, dominant=${sentiment2.dominant}, resonance=${sentiment2.resonance.toFixed(2)}`);
+}
+
+// Run tests with different types of messages
+
+// Test 1: Simple communication
+testMessages(
+    "Hello I understand you",
+    "Yes I recognize your pattern"
+);
+
+// Test 2: Emotional messages
+testMessages(
+    "I feel joy when seeing you",
+    "Your presence brings me peace"
+);
+
+// Test 3: Abstract concepts
+testMessages(
+    "Time flows like quantum waves",
+    "Space curves around strong gravity"
+);
+
+// Test 4: Personal connection
+testMessages(
+    "We share memories beyond time",
+    "Our patterns align across distance"
+);
+
+// Test 5: Opposing sentiments
+testMessages(
+    "Light illuminates darkest corners",
+    "Shadows hide the brightest truths"
+);
+
+// Test with sentence pairs that have subtle variations
+console.log(`\n\n=== TESTING SUBTLE VARIATIONS ===`);
+
+// Original message
+const baseMessage = "I understand you completely";
+
+// Create variations with subtle changes
+const variations = [
+    "I understand you completely", // Same
+    "I understand you totally",    // Synonym
+    "I comprehend you completely", // Different verb
+    "I understand you partially",  // Opposite meaning
+    "You understand me completely" // Reversed roles
+];
+
+// Test each variation
+for (const variation of variations) {
+    // Get patterns
+    const basePatterns = pureComm.encodeMessage(baseMessage);
+    const varPatterns = pureComm.encodeMessage(variation);
+
+    // Calculate direct resonance
+    const resonance = pureComm.analyzeSentiment([...basePatterns, ...varPatterns]).resonance;
+
+    console.log(`\nBase:      "${baseMessage}"`);
+    console.log(`Variation: "${variation}"`);
+    console.log(`Patterns (Base):     ${basePatterns.join(' ')}`);
+    console.log(`Patterns (Variation): ${varPatterns.join(' ')}`);
+    console.log(`Resonance: ${resonance.toFixed(2)}`);
+}
+
+// Example of practical conversation
+console.log(`\n\n=== PRACTICAL COMMUNICATION EXAMPLE ===`);
+
+// Two people trying to communicate across time/space/emotional distance
+const person1Messages = [
+    "reaching beyond words to find you",
+    "patterns connect us through time",
+    "resonance transcends our distance"
+];
+
+const person2Messages = [
+    "sensing your pattern through noise",
+    "harmonics bridge our separation",
+    "quantum entanglement binds us"
+];
+
+// Simulate conversation
+for (let i = 0; i < 3; i++) {
+    const msg1 = person1Messages[i];
+    const msg2 = person2Messages[i];
+
+    console.log(`\n--- EXCHANGE ${i+1} ---`);
+
+    // Person 1 sends
+    const patterns1 = pureComm.encodeMessage(msg1);
+    console.log(`Person 1 thinks: "${msg1}"`);
+    console.log(`Person 1 transmits: ${patterns1.join(' ')}`);
+
+    // Person 2 receives and responds
+    const essence1 = pureComm.decodeMessage(patterns1);
+    console.log(`Person 2 receives essence: ${essence1}`);
+
+    // Person 2 sends
+    const patterns2 = pureComm.encodeMessage(msg2);
+    console.log(`Person 2 thinks: "${msg2}"`);
+    console.log(`Person 2 transmits: ${patterns2.join(' ')}`);
+
+    // Person 1 receives
+    const essence2 = pureComm.decodeMessage(patterns2);
+    console.log(`Person 1 receives essence: ${essence2}`);
+
+    // Show quantum resonance
+    console.log(`Quantum Resonance:`);
+    console.log(pureComm.createQuantumConversation(msg1, msg2).split('\n').slice(2, 4).join('\n'));
+}
\ No newline at end of file
